<div class="layout-container">
<header role="banner">
{% include '@helpet/partials/header.html.twig' %}
</header>

   {{ page.primary_menu }}

   {{ page.secondary_menu  }}
   {{ page.breadcrumb }}
   {{ page.highlighted }}

   <main role="main">
    <div class="container ">
     <div class="row text-center">
       <div class="col-md-12">
          {{ page.content }}
        </div>
       </div>
     </div>

       {{ page.sidebar_first }}
       {{ page.sidebar_second }}
     {% if page.footer %}
       <div class="layaud-banner-top" role="contentinfo">
           {{ page.footer }}
       </div>
      {% endif %}
   </main>

</div>
